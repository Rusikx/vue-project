{
  "name": "vue-project-api",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "type1": "module",
  "engines": {
    "node": "21.7.*"
  },
  "main": "server.ts",
  "scripts": {
    "api": "node --no-warnings=ExperimentalWarning --loader ts-node/esm server.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "keywords": [
    "node js",
    "jwt",
    "authentication",
    "express",
    "postgresql"
  ],
  "dependencies": {
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "vee-validate": "^4.12.7",
    "ws": "^8.17.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "cypress": "^13.7.2",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.0"
  }
}
